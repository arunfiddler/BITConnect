<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/aero"
            android:layout_width="115sp"
            android:layout_height="200sp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"
            app:srcCompat="@drawable/aero" />

        <ImageButton
            android:id="@+id/agri"
            android:layout_width="115sp"
            android:layout_height="200sp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"
            android:layout_column="2"
            android:layout_row="0"
            app:srcCompat="@drawable/agri" />
        <ImageButton
            android:id="@+id/auto"
            android:layout_width="115sp"
            android:layout_height="200sp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="20dp"
            android:layout_column="3"
            android:layout_row="0"
            app:srcCompat="@drawable/auto" />
        <ImageButton
            android:id="@+id/bt"
            android:layout_width="115sp"
            android:layout_height="200sp"
            android:layout_marginLeft="10dp"

            android:layout_marginTop="20dp"
            android:layout_column="0"
            android:layout_row="1"
            app:srcCompat="@drawable/bt" />
        <ImageButton
            android:id="@+id/chem"
            android:layout_width="115sp"
            android:layout_height="200sp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="20dp"
            android:layout_column="2"
            android:layout_row="1"
            app:srcCompat="@drawable/chem" />
        <ImageButton
            android:id="@+id/civil"
            android:layout_width="115sp"
            android:layout_height="200sp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="20dp"
            android:layout_column="3"
            android:layout_row="1"
            app:srcCompat="@drawable/civil" />
        <ImageButton
            android:id="@+id/cse"
            android:layout_width="115sp"
            android:layout_height="200sp"
            android:layout_marginLeft="10dp"

            android:layout_marginTop="20dp"
            android:layout_column="0"
            android:layout_row="2"
            app:srcCompat="@drawable/cse" />
        <ImageButton
            android:id="@+id/ece"
            android:layout_width="115sp"
            android:layout_height="200sp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="20dp"
            android:layout_column="2"
            android:layout_row="2"
            app:srcCompat="@drawable/ece" />
        <ImageButton
            android:id="@+id/eee"
            android:layout_width="115sp"
            android:layout_height="200sp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="20dp"
            android:layout_column="3"
            android:layout_row="2"
            app:srcCompat="@drawable/eee" />

    </GridLayout>
</android.support.constraint.ConstraintLayout>