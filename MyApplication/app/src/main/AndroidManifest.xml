<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arunfiddler.sdc.myapplication">

    <uses-sdk
        android:minSdkVersion="15"
        android:maxSdkVersion="27"
        android:targetSdkVersion="26" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.arunfiddler.sdc.myapplication.Splash"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.arunfiddler.sdc.myapplication.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.arunfiddler.sdc.myapplication.Camps"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.arunfiddler.sdc.myapplication.Camps" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.arunfiddler.sdc.myapplication.developer"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.arunfiddler.sdc.myapplication.dept"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.arunfiddler.sdc.myapplication.dept" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.arunfiddler.sdc.myapplication.Listall" />
        <activity android:name="com.arunfiddler.sdc.myapplication.Results" />
        </application>

</manifest>